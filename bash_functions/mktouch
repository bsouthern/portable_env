#! /bin/bash
# TODO: This shit doesn't even work
# Create a file from a path and create directories as needed.
mktouch() {
    if [ $# -lt 1 ]; then
        echo "Missing argument";
        return 1;
    fi

    for f in "$@"; do
        mkdir -p -- "$(dirname -- "$f")"
        touch -- "$f"
    done
}
